function scrollUpDown(){
  var lS = 0;
  window.onscroll = function() {
    var cS = document.documentElement.scrollTop || document.body.scrollTop;
    if (cS > 155 && lS <= cS){
      lS = cS;
			if (cS > 300){
				if(document.body.classList.contains("udOneTransition")===false) {
					document.body.classList.add("udOneTransition");
				}
			}
			if(document.body.classList.contains("udTransition")===false) {
				document.body.classList.add("udTransition");
			}
    }else{
      lS = cS;
			if(cS <= 155) {
				if(document.body.classList.contains("udOneTransition")) {
					document.body.classList.remove("udOneTransition")
				}
			}
			if(document.body.classList.contains("udTransition")) {
				document.body.classList.remove("udTransition");
			}
    }
	};
}
