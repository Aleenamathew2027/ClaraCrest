	  return false;
	}
}

function setExpireCatalogPopSteTime() {
	//var limit = 1 * 24 * 60 * 60 * 1000;
	var limit = 1 * 60 * 60 * 1000;
	return new Date().getTime() + limit;
}

function newSubscribeCatalogCallPopup() {
	/* */ cnscbCatalogEthOpen(); 
}

function newCatalogSubscribePopup() {

	if(localStorage.getItem('nscpCatalogPopSte') != 'nschown' || expireCatalogPopSteTime()){ 
		setTimeout(function(){ newSubscribeCatalogCallPopup(); }, 30000);
		localStorage.setItem('nscpCatalogPopSte','nschown');		
		localStorage.setItem('expireCatalogPopSteTime', setExpireCatalogPopSteTime());	
	}
	
}

