function cnscbCatalogEthOpen() {
	document.getElementById('r_client_id').value  = localStorage.getItem("gclid");
	document.getElementById('cnscbEth').classList.add("open");
	document.getElementById('cnscbCatalogEthOvrly').classList.add("show");
}
function cnscbEthClose() {
	if(document.getElementById('cnscbEth').classList.contains("open")) { 
		document.getElementById('cnscbEth').classList.remove("open");
		document.getElementById('cnscbCatalogEthOvrly').classList.remove("show");
	}
}
